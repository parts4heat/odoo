<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_exploded_view_form" model="ir.ui.view">
        <field name="name">exploded.view.form</field>
        <field name="model">exploded.view</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="binary"/>
                        <field name="file_name" invisible="1"/>
                        <field name="model_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>

