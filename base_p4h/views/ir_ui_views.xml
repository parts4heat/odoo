<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <record model="ir.ui.view" id="view_shipstation_postback_tree">
      <field name="name">x_gfp.shipstation.postback.tree</field>
      <field name="model">shipstation.postback</field>
      <field name="inherit_id" eval="False"/>
      <field name="priority">16</field>
      <field name="type">tree</field>
      <field name="mode">primary</field>
      <field name="arch" type="xml">
        <tree string="Posts">
          <field name="create_date"/>
          <field name="name"/>
          <field name="success"/>
          <button name="post_shipstation" type="object" string="POST"/>
        </tree>
      </field>
    </record>
    <record model="ir.ui.view" id="view_shipstation_postback_form">
          <field name="name">x_gfp.shipstation.postback.form</field>
          <field name="model">shipstation.postback</field>
          <field name="inherit_id" eval="False"/>
          <field name="priority">16</field>
          <field name="groups_id" eval="[(6, 0, [])]"/>
          <field name="type">form</field>
          <field name="mode">primary</field>
          <field name="arch" type="xml">
            <form>
              <sheet>
                <group>
                  <group>
                    <field name="name"/>
                    <field name="success"/>
                  </group>
                  <group>
                    <field name="tracking"/>
                    <field name="cost"/>
                    <field name="order_num"/>
                    <field name="order_id"/>
                    <field name="order_key"/>
                    <field name="service_code"/>
                    <field name="ship_to"/>
                  </group>
                </group>
                <group>
                  <field name="returned_dictionary"/>
                </group>
                <group>
                  <field name="exception"/>
                </group>
              </sheet>
            </form>
          </field>
    </record>
    <record model="ir.ui.view" id="view_shipstation_package_tree">
      <field name="name">x_gfp.delivery.shipstation.package.tree</field>
      <field name="model">delivery.shipstation.package</field>
      <field name="inherit_id" eval="False"/>
      <field name="priority">16</field>
      <field name="type">tree</field>
      <field name="mode">primary</field>
      <field name="arch" type="xml">
        <tree string="Posts">
          <field name="name"/>
          <field name="code"/>
          <field name="carrier_code"/>
          <field name="price"/>
          <field name="delivery_length"/>
          <field name="delivery_width"/>
          <field name="delivery_height"/>
          <field name="package_weight"/>
          <field name="inner_delivery_length"/>
          <field name="inner_delivery_width"/>
          <field name="inner_delivery_height"/>
          <field name="max_package_weight"/>
        </tree>
      </field>
    </record>
    <record model="ir.ui.view" id="view_shipstation_package_form">
      <field name="name">x_gfp.delivery.shipstation.package.form</field>
      <field name="model">delivery.shipstation.package</field>
      <field name="inherit_id" eval="False"/>
      <field name="priority">16</field>
      <field name="type">form</field>
      <field name="mode">primary</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <group>
                <field name="code"/>
                <field name="name"/>
                <field name="carrier_code"/>
                <field name="domestic"/>
                <field name="international"/>
                <field name="created_by_shipstation"/>
                <field name="price"/>
              </group>
              <group>
                <field name="delivery_length"/>
                <field name="delivery_width"/>
                <field name="delivery_height"/>
                <field name="package_weight"/>
                <field name="inner_delivery_length"/>
                <field name="inner_delivery_width"/>
                <field name="inner_delivery_height"/>
                <field name="max_package_weight"/>
              </group>
            </group>
          </sheet>
        </form>
      </field>
    </record>
    <record model="ir.ui.view" id="view_picking_form_mod">
      <field name="name">x_gfp.stock.picking.form.mod</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="stock.view_picking_form"/>
      <field name="priority">98</field>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='move_line_ids_without_package']//tree//field[@name='product_id']" position="after">
            <field name="storage_location_id"
            attrs="{
                'column_invisible': [('parent.picking_type_code', '=','outgoing')],
            }"/>
          </xpath>
          <xpath expr="//field[@name='move_ids_without_package']//tree//field[@name='product_id']" position="after">
            <field name="storage_location_id"
            attrs="{
                'column_invisible': [('parent.picking_type_code', '=','outgoing')],
            }"/>
          </xpath>
          <xpath expr="//field[@name='partner_id']" position="before">
            <label for="ss_id" attrs="{'invisible':[('ss_id','=',0)]}"/>
            <div attrs="{'invisible':[('ss_id','=',0)]}">
              <field name="ss_id" readonly="1" attrs="{'invisible':[('ss_id','=',0)]}"/> |
              <field name="ss_status" readonly="1" attrs="{'invisible':[('ss_id','=',0)]}"/> |
              <field name="ss_address_status" readonly="1" attrs="{'invisible':[('ss_id','=',0)]}"/> |
              <button name="delete_ssorder" string="Cancel" type="object" attrs="{'invisible':[('ss_status','=','cancelled')]}" invisible="1"/>
            </div>
            <field name="ss_ship_id" attrs="{'invisible':[('ss_ship_id','=',0)]}"/>
          </xpath>
          <xpath expr="//field[@name='origin']" position="after">
            <field name="carrier_id" options="{'no_create': True, 'no_open': True}"/>
            <field name="delivery_carrier_code" invisible="1"/>
            <field name="delivery_package"/>
            <label string="Dimensions" for="delivery_length"/>
            <div class="o_address_format">
              <field name="delivery_length" class="o_address_zip"/>
              <field name="delivery_width" class="o_address_zip"/>
              <field name="delivery_height" class="o_address_zip"/>
            </div>
            <field name="client_account" domain="[('partner_id','=',partner_id)]"/>
            <field name="confirmation"/>
            <field name="delivery_type" attrs="{'invisible':True}"/>
            <field name="carrier_tracking_ref" class="oe_inline" attrs="{'readonly': [('state', '=', 'done')]}"/>
          </xpath>
          <xpath expr="//field[@name='backorder_id']" position="after">
            <field name="sale_id" readonly="1"/>
            <field name="purchase_id" readonly="1"/>
          </xpath>
          <xpath expr="//header" position="inside">
            <button name="create_update_ssorder" string="Send to Shipstation" type="object" class="oe_highlight" attrs="{'invisible':['|',('ss_id','!=',0),('state','not in',['done'])]}"/>
          </xpath>
        </data>
      </field>
    </record>
    <record model="ir.ui.view" id="mrp_bom_form_view_mod">
      <field name="name">x_gfp.mrp.bom.form.mod</field>
      <field name="model">mrp.bom</field>
      <field name="inherit_id" ref="mrp.mrp_bom_form_view"/>
      <field name="priority">98</field>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='bom_line_ids']//tree//field[@name='product_id']" position="after">
            <field name="standard_price"/>
            <field name="qty_available"/>
          </xpath>
        </data>
      </field>
    </record>
    <record model="ir.ui.view" id="product_template_form_view">
      <field name="name">x_gfp.product.template.form.mod</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="product.product_template_form_view"/>
      <field name="priority">98</field>
      <field name="arch" type="xml">
        <data>
        <xpath expr="//header" position="after">
            <field name="bom_count" invisible="1"/>
            <div align="center" style="background-color:#FFA500;" attrs="{'invisible':[('bom_count','=',0)]}">
              <div>
                <div>
                <strong>NOTE: This part can be manufactured in-house</strong>
                </div>
              </div>
            </div>
          </xpath>
          <xpath expr="//group[@name='group_lots_and_weight']/label" position="before">
            <field name="storage_location_id"/>
          </xpath>
        </data>
      </field>
    </record>
    <record model="ir.ui.view" id="product_template_form_view_procurement_button_mod">
      <field name="name">x_gfp.product.template.procurement.form.mod</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="stock.product_template_form_view_procurement_button"/>
      <field name="priority">98</field>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='qty_available']" position="replace">
            <field name="qty_available" invisible="1"/>
            <field name="ph_qty_available" widget="statinfo" nolabel="1" class="mr4"/>
          </xpath>
          <xpath expr="//field[@name='virtual_available']" position="replace">
            <field name="virtual_available" invisible="1"/>
            <field name="ph_virtual_available" widget="statinfo" nolabel="1" class="mr4"/>
          </xpath>
          <xpath expr="//field[@name='qty_available_not_res']/../../.." position="attributes">
            <attribute name="name">action_open_quants_unreserved</attribute>
          </xpath>
          <xpath expr="//field[@name='virtual_available']/../../.." position="attributes">
            <attribute name="name">action_open_ph_forecast</attribute>
            <attribute name="type">object</attribute>
          </xpath>
        </data>
      </field>
    </record>
    <record model="ir.ui.view" id="view_order_form_with_carrier">
      <field name="name">x_gfp.delivery.sale.order.form.view.with_carrier.mod</field>
      <field name="model">sale.order</field>
      <field name="inherit_id" ref="delivery.view_order_form_with_carrier"/>
      <field name="priority">99</field>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='carrier_id']" position="after">
            <button name="get_shipstation_rates" string="Get Shipstation Rates" type="object" class="oe_inline fa fa-arrow-right oe_link" attrs="{'invisible':['|',('carrier_id','!=',False),('state','not in',('draft','sent'))]}"/>
          </xpath>
          <xpath expr="//button[@name='get_delivery_price']" position="attributes">
            <attribute name="invisible">1</attribute>
          </xpath>
          <xpath expr="//button[@name='set_delivery_line']" position="attributes">
            <attribute name="attrs">{'invisible':['|','|',('carrier_id','=',False),('state','not in',('draft','sent')),('delivery_rating_success','=',False)]}</attribute>
          </xpath>
          <xpath expr="//field[@name='payment_term_id']" position="after">
          <button name="get_delivery_price" string="Check price" type="object" class="oe_inline fa fa-arrow-right oe_link"/>
          <field name="client_account" domain="[('partner_id','=',partner_id)]" string="Delivery Account"/>
          <label for="delivery_package"/>
          <div>
            <div>
              <field name="delivery_package" string="Delivery Package" options="{'no_create': True}"/>
            </div>
          </div>
          <field name="confirmation"/>
          <label string="Dimensions (Inches)" for="delivery_length"/>
          <div class="o_address_format">
            <field name="delivery_length" class="o_address_zip"/> x <field name="delivery_width" class="o_address_zip"/> x <field name="delivery_height" class="o_address_zip"/>
          </div>
          <field name="delivery_carrier_code" invisible="1"/>
        </xpath>
        </data>
      </field>
    </record>

    <record model="ir.ui.view" id="view_order_form_mod">
      <field name="name">x_gfp.sale.order.form.mod</field>
      <field name="model">sale.order</field>
      <field name="inherit_id" ref="sale.view_order_form"/>
      <field name="priority">98</field>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='date_order']" position="after">
            <field name="shipdate"/>
            <field name="shipresidential" invisible="1"/>
          </xpath>
        </data>
      </field>
    </record>
    <record model="ir.ui.view" id="view_partner_form_mod">
      <field name="name">x_gfp.res.partner.form.mod</field>
      <field name="model">res.partner</field>
      <field name="inherit_id" ref="base.view_partner_form"/>
      <field name="priority">98</field>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='ref']" position="after">
            <field name="vendor_id"/>
            <field name="customerid"/>
          </xpath>
          <xpath expr="//field[@name='mobile']/.." position="after">
              <label for="fax"/>
              <div class="o_row">
                 <field name="fax" widget="phone"/>
              </div>
          </xpath>
          <xpath expr="//field[@name='property_delivery_carrier_id']" position="after">
            <field name="residential"/>
            <label for="shipstation_warehouse_id"/>
            <div name="ship_warehouse">
              <div>
                <field name="shipstation_warehouse_id" readonly="1" class="oe_inline"/>
              </div>
              <div>
                <button name="create_shipstation_warehouse_id" string="Create/Update Warehouse" type="object" class="oe_inline fa fa-arrow-right oe_link"/>
              </div>
            </div>
          </xpath>
          <xpath expr="//notebook" position="inside">
            <page string="Shipping Accounts" attrs="{'invisible':[('parent_id','!=',False)]}">
              <group>
                <field name="shipping_account_ids" nolabel="1" widget="one2many_list">
                  <tree editable="bottom">
                    <field name="partner_id" invisible="1"/>
                    <field name="carrier_id"/>
                    <field name="name"/>
                    <field name="zip"/>
                  </tree>
                </field>
              </group>
            </page>
          </xpath>
        </data>
      </field>
    </record>
    <record model="ir.ui.view" id="view_delivery_carrier_form_mod">
      <field name="name">x_gfp.delivery.carrier.form.mod</field>
      <field name="model">delivery.carrier</field>
      <field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
      <field name="priority">98</field>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//field[@name='delivery_type']/../.." position="inside">
            <field name="shippingmethodid"/>
          </xpath>
          <xpath expr="//field[@name='company_id']" position="after">
              <field name="service_code"/>
              <field name="carrier_code"/>
              <field name="domestic"/>
              <field name="international"/>
          </xpath>
        </data>
      </field>
    </record>
    <record model="ir.ui.view" id="res_config_settings_view_form_mod">
      <field name="name">x_gfp.res.config.settings.mod</field>
      <field name="model">res.config.settings</field>
      <field name="inherit_id" ref="base.res_config_settings_view_form"/>
      <field name="priority">200</field>
      <field name="arch" type="xml">
        <data>
          <xpath expr="//div[@id='multi_company']" position="after">
          <div id="shipstation_integration">
              <h2>Shipstation</h2>
              <div class="row mt16 o_settings_container">
                  <div class="col-xs-12 col-md-6 o_setting_box">
                      <div class="o_setting_right_pane">
                          <label string="API Key" for="shipstation_key"/>
                          <span class="fa fa-lg fa-building-o" title="Values set here are company-specific." groups="base.group_multi_company"/>
                          <div class="text-muted">
                              Please enter the API key for Shipstation
                          </div>
                          <div class="content-group">
                              <div class="mt16 row">
                                  <label for="shipstation_key" string="Key" class="col-xs-3 col-md-3 o_light_label"/>
                                  <field name="shipstation_key" class="oe_inline"/>
                              </div>
                          </div>
                      </div>
                      <div class="o_setting_right_pane">
                          <label string="API Secret" for="shipstation_secret"/>
                          <span class="fa fa-lg fa-building-o" title="Values set here are company-specific." groups="base.group_multi_company"/>
                          <div class="text-muted">
                              Please enter the API secret for Shipstation
                          </div>
                          <div class="content-group">
                              <div class="mt16 row">
                                  <label for="shipstation_secret" string="Secret" class="col-xs-3 col-md-3 o_light_label"/>
                                  <field name="shipstation_secret" class="oe_inline"/>
                              </div>
                          </div>
                      </div>
                      <div class="o_setting_right_pane">
                          <label string="Shipstation Webhooks" for="shipstation_hook_ids"/>
                          <div class="text-muted">
                              Live Post-Back Service Webhooks
                          </div>
                      </div>
                      <div class="content-group" style="margin-left:50px;">
                          <div class="row" width="100%">
                            <field name="shipstation_hook_ids" nolabel="1" readonly="1" widget="one2many_list">
                              <tree>
                                <field name="company_id" invisible="1"/>
                                <field name="web_hook_id"/>
                                <field name="name"/>
                                <field name="url"/>
                                <field name="hook_type"/>
                                <field name="active"/>
                                <button name="delete_webhook" icon="fa-times-circle text-danger" type="object"/>
                              </tree>
                            </field>
                          </div>
                      </div>
                  </div>
                  <div class="col-xs-12 col-md-6 o_setting_box">
                      <div class="o_setting_right_pane">
                          <label string="Endpoint" for="shipstation_root_endpoint"/>
                          <span class="fa fa-lg fa-building-o" groups="base.group_multi_company"/>
                          <div class="text-muted">
                              Please enter root endpoint for Shipstation
                          </div>
                          <div class="content-group">
                              <div class="mt16 row">
                                  <label for="shipstation_root_endpoint" string="URL" class="col-xs-3 col-md-3 o_light_label"/>
                                  <field name="shipstation_root_endpoint" class="oe_inline"/>
                              </div>
                          </div>
                      </div>
                      <div class="o_setting_right_pane">
                          <label string="Post-Back Service" for="target_postback_url"/>
                          <span class="fa fa-lg fa-building-o" title="Values set here are company-specific." groups="base.group_multi_company"/>
                          <div class="text-muted">
                              Please enter the post-back endpoint for confirmation of order
                          </div>
                          <div class="content-group">
                              <div class="mt16 row">
                                  <label for="target_postback_url" string="URL" class="col-xs-3 col-md-3 o_light_label"/>
                                  <field name="target_postback_url" placeholder="e.g https://www.odoo.com/shipstation/shipped" class="oe_inline"/>
                              </div>
                          </div>
                      </div>
                      <div class="o_setting_right_pane">
                          <label string="Update Shipstation Information" for="get_service"/>
                          <span class="fa fa-lg fa-building-o" title="Values set here are company-specific." groups="base.group_multi_company"/>
                          <div class="text-muted">
                              Updating these elements will update existing records and create if not found in the Odoo database
                          </div>
                          <div class="content-group">
                              <div class="mt16">
                                  <button name="get_service" string="Create/Update Delivery Services" type="object" class="oe_link"/>
                                  <button name="get_packages" string="Create/Update Delivery Packages" type="object" class="oe_link"/>
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
            </div>
              <div id="ftp_integration">
                <h2>FTP Order &amp; Image Integration</h2>
                <div class="row mt16 o_settings_container" name="ftp_integration">
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="image_ftp_address"/>
                            <div class="text-muted">
                                Input the FTP address for the images
                            </div>
                            <div class="content-group">
                                <field name="image_ftp_address"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="image_ftp_user"/>
                            <div class="text-muted">
                                Input the FTP user for the images
                            </div>
                            <div class="content-group">
                                <field name="image_ftp_user"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="image_ftp_password"/>
                            <div class="text-muted">
                                Input the FTP password for the images
                            </div>
                            <div class="content-group">
                                <field name="image_ftp_password" password="1"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="image_ftp_port"/>
                            <div class="text-muted">
                                Input the FTP port for the images
                            </div>
                            <div class="content-group">
                                <field name="image_ftp_port"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="image_ftp_folder"/>
                            <div class="text-muted">
                                Input the FTP folder for the images
                            </div>
                            <div class="content-group">
                                <field name="image_ftp_folder"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="image_ftp_type"/>
                            <div class="text-muted">
                                Image FTP Type
                            </div>
                            <div class="content-group">
                                <field name="image_ftp_type"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="image_ftp_last_update"/>
                            <div class="text-muted">
                                Last time the image creation cron ran
                            </div>
                            <div class="content-group">
                                <field name="image_ftp_last_update"/>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mt16 o_settings_container" name="order_ftp_integration">
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="order_ftp_address"/>
                            <div class="text-muted">
                                Input the FTP address for the order
                            </div>
                            <div class="content-group">
                                <field name="order_ftp_address"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="order_ftp_user"/>
                            <div class="text-muted">
                                Input the FTP user for the orders
                            </div>
                            <div class="content-group">
                                <field name="order_ftp_user"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box">
                        <div class="o_setting_left_pane">
                        </div>
                        <div class="o_setting_right_pane">
                            <label for="order_ftp_password"/>
                            <div class="text-muted">
                                Input the FTP password for the orders
                            </div>
                            <div class="content-group">
                                <field name="order_ftp_password" password="1"/>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-lg-6 o_setting_box">
