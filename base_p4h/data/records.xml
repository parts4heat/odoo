<?xml version="1.0" encoding='UTF-8'?>
<odoo>
    <record forcecreate="True" id="unmapped_delivery_carrier" model="delivery.carrier">
        <field name="name">Unmapped Volusion Carrier</field>
        <field name="delivery_type">fixed</field>
        <field name="product_id" ref="delivery.product_product_delivery"/>
        <field name="fixed_price">0.00</field>
    </record>
    <record forcecreate="True" id="unmapped_payment_term" model="account.payment.term">
        <field name="name">Unmapped Payment Term</field>
    </record>
    <record id="shipstation_package_default" model="delivery.shipstation.package">
      <field name="code">package</field>
      <field name="name">Default Package</field>
      <field name="domestic" eval="True"/>
      <field name="international" eval="True"/>
    </record>
</odoo>
